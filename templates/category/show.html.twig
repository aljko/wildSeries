
{% extends'base.html.twig' %}
{% block body %}
 <h1 class="mt-0">{{ category.name }}</h1>
 {% for program in programs %}
  <ul>
   <li>
    <h2><a href="{{ path("program_show", { id: program.id }) }}">{{ program.id }} / {{ program.title }}</a></h2>
    <p>{{ program.summary }}</p>
    
    
   </li>
  </ul>
 {% else %}
  Aucune série trouvée.
 {% endfor %}
 <a href="{{ path('category_index') }}">
  Retour à la liste des catégories
 </a>

{% endblock %}